@model IEnumerable<ENTIDAD.Contenido>
@{ var contenido = (IEnumerable<ENTIDAD.Contenido>)ViewData["contenidoVista"];
    ViewBag.Title = "Contacto";
}

    <div class="container ">
        <div class="row">
            <div class="col-3"></div>
            <div class="col-6">

                @foreach (var dato in contenido)
                {



                    <hr style="color: #47748b">
                    <center>  <strong> @Html.DisplayFor(modelItem => dato.titulo)</strong></center>


                    <hr style="color: #47748b">
                    <br>
                    <div class="social-links mt-3 text-center" style="color:white">

                        <a href="#" class="twitter"><i class="bx bxl-twitter"></i></a>
                        <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
                        <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
                        <a href="#" class="google-plus"><i class="bx bxl-skype"></i></a>
                        <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
                    </div>
                    <br>
                    <div style="text-align: justify;"> @Html.DisplayFor(modelItem => dato.contenido) </div>

                }
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
            </div>
            <div class="col-3"></div>

        </div>
    </div>


<br>
<br>
<br>
