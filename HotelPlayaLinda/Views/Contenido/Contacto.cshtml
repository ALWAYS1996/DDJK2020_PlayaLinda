@model IEnumerable<ENTIDAD.Contenido>
@{ var contenido = (IEnumerable<ENTIDAD.Contenido>)ViewData["contenidoVista"];
    ViewBag.Title = "Contacto";
}

<div class="container ">
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">

            @foreach (var dato in contenido)
            {



                <hr style="color: #47748b">
                <font size="4"> <center>  <strong> @Html.DisplayFor(modelItem => dato.titulo)</strong></center></font>


                <hr style="color: #47748b">
                <br>
                <div class="social-links mt-3 text-center" style="color:white">

                    <a href="#" class="twitter"><i class="bx bxl-twitter"></i></a>
                    <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
                    <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
                    <a href="#" class="google-plus"><i class="bx bxl-skype"></i></a>
                    <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
                </div>
                <br>
                <div style="text-align: justify;"> @Html.DisplayFor(modelItem => dato.contenido) </div>

            }

            <br>

            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d125736.54585455992!2d-84.87632374476418!3d9.994785075118306!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x76e696288384f4c6!2sHotel%20Sol%20Puntarenas!5e0!3m2!1sen!2scr!4v1593213538845!5m2!1sen!2scr" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>  <br>

            <br>
            <br>
            <br>
            <br>
            <br>
        </div>
        <div class="col-3"></div>

    </div>
</div>


<br>
