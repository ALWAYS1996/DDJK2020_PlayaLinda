<div class="col-4" style="margin: 25px;">
    @using (Html.BeginForm("GenerarReservacion", "Reserva", FormMethod.Post))
    {
        <input type="text" class="form-control" id="codigoTipoHabitacion" value="@ViewData["codigoTipoHabitacion"]" name="codigoTipoHabitacion" >
        <input type="text" class="form-control" id="fechaInicio" value="@ViewData["fechaInicio"]" name="fechaInicio" >
        <input type="text" class="form-control" id="fechaFin" value="@ViewData["fechaFin"]" name="fechaFin" >
        <div class="form-group">
            <label for="exampleInputPassword1">Pasaporte</label>
            <input type="text" class="form-control" id="pasaporte" name="pasaporte">
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1">Nombre</label>
            <input type="text" class="form-control" id="nombre" name="nombre">
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1">Apellido</label>
            <input type="text" class="form-control" id="apellido" name="apellido">
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1">Email</label>
            <input type="text" class="form-control" id="email" name="email">
        </div>

        <div class="form-group">
            <label for="exampleInputPassword1">Tarjeta</label>
            <input type="text" class="form-control" id="tarjeta" name="tarjeta">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    }
</div>